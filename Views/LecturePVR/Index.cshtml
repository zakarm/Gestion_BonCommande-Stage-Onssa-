@model IEnumerable<Projet_Onssa_Web_Mvc.Models.PVRSet>

@{
    ViewBag.Title = "Index";
}

<div style="padding:50px;">

    <h2>Lecture procès de reception </h2>
    <br />
    <br />

    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DatePVR)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumPvr)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InfoOI.NumOi)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DatePVR)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.NumPvr)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.InfoOI.NumOi)
                </td>
                <td>
                    @Html.ActionLink("Export", "Export", new { id = item.IdPVR })
                </td>
            </tr>
        }

    </table>
</div>
